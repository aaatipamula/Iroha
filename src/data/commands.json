{
    "search": {
        "hidden": false,
        "cmd_desc": "Search for an anime with keywords.",
        "params": [
            {
                "name": "content_type",
                "optional": true,
                "type": "positional",
                "data_type": "A type of media",
                "description": "What type of content you want returned.",
                "notes": "Valid Options: ova, ona, show, short, movie, special, music, manga, novel, oneshot\nDefaults to *show*"
            },
            {
                "name": "search_string",
                "optional": true,
                "type": "positional",
                "data_type": "A query",
                "description": "What anime you want to search."
            }
        ],
        "usage": [
            "`?search one piece`",
            "`?search movie naruto`",
            "`?search manga komi can't communicate`"
        ]
    },


    "seasonal": {
        "hidden": false,
        "cmd_desc": "Displays the top 3 seasonal anime from the current season.",
        "params": [
            {
                "name": "results",
                "optional": true,
                "type": "positional",
                "data_type": "number",
                "description": "Number of anime to be displayed.",
                "notes": "Defaults to 3"
            },
            {
                "name": "season",
                "optional": true,
                "type": "positional",
                "data_type": "A season (winter, spring, summer, fall)",
                "description": "What season of anime you want.",
                "notes": "Default is the current season."
            },
            {
                "name": "year",
                "optional": true,
                "type": "positional",
                "data_type": "A valid year.",
                "description": "The year of the anime season.",
                "notes": "Default is the current year"
            }
        ],
        "usage": [
            "`?seasonal`",
            "`?seasonal winter 2021`"
        ]
    },


    "ping": {
        "hidden": false,
        "cmd_desc": "Sends *pong* back",
        "params": null,
        "usage": [
            "`?ping`"
        ]
    }
}
